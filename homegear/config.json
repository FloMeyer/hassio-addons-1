{
  "name": "Homegear",
  "version": "0.79.1",
  "slug": "homegear",
  "description": "Homegear for Hassio add-on",
  "startup": "before",
  "boot": "auto",
  "url": "https://community.home-assistant.io/t/homegear-add-on-for-hassio/35169/10",
  "hassio_api": false,
  "homeassistant_api": false,
  "auto_uart": true,
  "webui": "[PROTO:ssl]://[HOST]:[PORT:8234]",
  "gpio": true,
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "map": [
    "config:rw", 
    "share:rw"
  ],
  "environment": {
  },
  "options": {
     "log_level": "info"  
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)"
  },
  "ports": {
    "2001": 2001,
    "2002": 2002,
    "2003": 2003
  },
}
