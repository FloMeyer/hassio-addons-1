{
  "name": "Homegear-Rpi",
  "version": "0.70.2",
  "slug": "homegear-rpi",
  "description": "Homegear as a Hassio add-on for RaspberryPi",
  "startup": "services",
  "boot": "auto",
  "gpio": true,
  "privileged": [
    "SYS_ADMIN",
    "SYS_RAWIO"
  ],
  "map": [
	  "config:rw", 
	  "share:rw"
  ],
  "ports": {
    "2001": 2001,
    "2002": 2002,
    "2003": 2003
  },
  "environment": {
  },
  "options": {
  },
  "schema": {
  },
  "devices": [
    "/dev/ttyAMA0:/dev/ttyAMA0:rwm",
    "/dev/ttyACM0:/dev/ttyACM0:rwm"
  ]
}
